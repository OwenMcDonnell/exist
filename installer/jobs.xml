<?xml version="1.0" encoding="UTF-8"?>
<izpack:processing xmlns:izpack="http://izpack.org/schema/processing" version="5.0">
    <logfiledir>$INSTALL_PATH</logfiledir>
    <job name="Setting admin password ...">
        <os family="windows"/>
        <executefile name="$INSTALL_PATH/bin/setup.bat">
            <arg>pass:$adminPasswd</arg>
        </executefile>
    </job>
    <job name="Setting admin password ...">
        <os family="unix"/>
        <executefile name="$INSTALL_PATH/bin/setup.sh">
            <arg>pass:$adminPasswd</arg>
        </executefile>
    </job>
</izpack:processing>
